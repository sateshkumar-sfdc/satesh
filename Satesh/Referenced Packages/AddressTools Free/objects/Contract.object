        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var linkId = &apos;ctrc25street_validatelnk&apos;; 
function clickLink(id) { 
    var link = document.getElementById(id);
    if (!link){ 
        return false;
    } 

    if(!!link &amp;&amp; link.click) { 
        link.click(); 
    } 
    return true;
} 

if (window.ProvenWorks === undefined) {
{!REQUIRESCRIPT(&quot;/apex/pw_ccpro__CountriesJavaScript?core.apexpages.devmode.url=1&quot;)}
{!REQUIRESCRIPT(&quot;/apex/pw_ccpro__SessionJavaScript?core.apexpages.devmode.url=1&quot;)}
{!REQUIRESCRIPT(&quot;/resource/pw_ccpro__CountryCompleteResources/javascript/CountryAutoComplete.js&quot;)}
{!REQUIRESCRIPT(&quot;/resource/pw_ccpro__AddressCompleteResources/javascript/AddressComplete.js&quot;)}
}

setTimeout(function run() {
  var clicked = clickLink(linkId);
  if (!clicked){
      setTimeout(run, 500);
  }
}, 500);</url>
